{block head}
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=3.0">
    <!-- <meta http-equiv="Content-Security-Policy" content="script-src 'none'"> -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-title" content="{\Ivy\Setting::getStash()['name']->getValue()}">
    <meta name="theme-color" content="#ffffff">
    <meta name="description" content="{\Ivy\Setting::getStash()['description']->getValue()}">
    <meta name="keywords" content="{\Ivy\Setting::getStash()['keywords']->getValue()}">
    <meta name="author" content="{\Ivy\Setting::getStash()['author']->getValue()}">
    <meta name="date.created" content="{\Ivy\Setting::getStash()['created']->getValue()}">
    <meta name="date.available" content="{\Ivy\Setting::getStash()['available']->getValue()}">
    <meta name="date.updated" content="{\Ivy\Setting::getStash()['updated']->getValue()}">
    <meta name="robots" content="index, follow">
    <meta name="revisit-after" content="3 days">
    <meta property="og:title" content="{\Ivy\Setting::getStash()['title']->getValue()}">
    <meta property="og:site_name" content="{\Ivy\Setting::getStash()['name']->getValue()}">
    <meta property="og:url" content="{\Ivy\Setting::getStash()['url']->getValue()}">
    <meta property="og:description" content="{\Ivy\Setting::getStash()['description']->getValue()}">
    <meta property="og:type" content="website">
    <meta property="og:locale" content="{\Ivy\Setting::getStash()['language']->getValue()}"/>
    <meta property="og:image"
          content="{!isset(\Ivy\Setting::getStash()['image']) ?: \Ivy\Setting::getStash()['image']?->getValue()}"/>
    <meta name="twitter:card" content="summary">
    <meta name="twitter:title" content="{\Ivy\Setting::getStash()['title']->getValue()}">
    <meta name="twitter:description" content="{\Ivy\Setting::getStash()['description']->getValue()}">
    <meta name="twitter:image"
          content="{!isset(\Ivy\Setting::getStash()['image']) ?: \Ivy\Setting::getStash()['image']?->getValue()}">

    <link rel="shortcut icon" type="image/x-icon" href="{= _p('BASE_PATH') . _p('MEDIA_PATH') . 'favicon/favicon.ico'}">
    <link rel="shortcut icon" type="image/gif" href="{= _p('BASE_PATH') . _p('MEDIA_PATH') . 'favicon/favicon.gif'}">
    <link rel="shortcut icon" type="image/png" href="{= _p('BASE_PATH') . _p('MEDIA_PATH') . 'favicon/favicon.png'}">
